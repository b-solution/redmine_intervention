<p id="notice"><%= notice %></p>

<h1>Interventions</h1>

<table>
  <thead>
    <tr>
      <th>Issue</th>
      <th>User</th>
      <th>Batiment</th>
      <th>Site</th>
      <th>Nom demandeur</th>
      <th>Date reclamation</th>
      <th>Date intevention</th>
      <th>Equipe</th>
      <th>Travaux</th>
      <th>Nature intervention</th>
      <th>Description travaux</th>
      <th>Founitures</th>
      <th>Oberservation</th>
      <th>Travaux termines</th>
      <th>Remarque</th>
      <th>Signature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interventions.each do |intervention| %>
      <tr>
        <td><%= intervention.issue_id %></td>
        <td><%= intervention.user_id %></td>
        <td><%= intervention.batiment %></td>
        <td><%= intervention.site %></td>
        <td><%= intervention.nom_demandeur %></td>
        <td><%= intervention.date_reclamation %></td>
        <td><%= intervention.date_intevention %></td>
        <td><%= intervention.equipe %></td>
        <td><%= intervention.travaux_id %></td>
        <td><%= intervention.nature_intervention_id %></td>
        <td><%= intervention.description_travaux %></td>
        <td><%= intervention.founitures %></td>
        <td><%= intervention.oberservation %></td>
        <td><%= intervention.travaux_termines %></td>
        <td><%= intervention.remarque %></td>
        <td><%= intervention.signature %></td>
        <td><%= link_to 'Show', intervention %></td>
        <td><%= link_to 'Edit', edit_intervention_path(intervention) %></td>
        <td><%= link_to 'Destroy', intervention, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intervention', new_intervention_path %>
